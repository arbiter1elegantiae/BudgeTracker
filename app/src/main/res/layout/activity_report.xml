<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.senso.budgetracker.reportActivity"
    android:id="@+id/activity_reportt">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Snapshot delle spese"
        android:textSize="30dp"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:id="@+id/textView4" />



    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/textView4"/>

    <TextView
        android:id="@+id/content_nexpense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/date"
        android:layout_marginTop="27dp"
        android:layout_marginLeft="10dp"
        />



    <TextView
        android:id="@+id/content_plexpense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/content_nexpense"
        android:layout_marginTop="27dp"
        android:layout_marginLeft="10dp"
        />


    <TextView
        android:id="@+id/content_pexpense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/content_plexpense"
        android:layout_marginTop="27dp"
        android:layout_marginLeft="10dp"
        />


</RelativeLayout>
